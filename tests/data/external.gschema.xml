<!-- Include here the external schemas we depend and need for testing -->

<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/apps/indicator-session/" id="com.canonical.indicator.session">
    <key type="b" name="suppress-logout-restart-shutdown">
      <default>false</default>
    </key>
  </schema>

  <schema id="org.compiz.unityshell" gettext-domain="compiz">
    <key type="s" name="panel-first-menu">
      <default>'&lt;Alt&gt;F10'</default>
    </key>
    <key type="s" name="show-launcher">
      <default>'&lt;Super&gt;'</default>
    </key>
    <key type="s" name="show-hud">
      <default>'&lt;Alt&gt;'</default>
    </key>
   </schema>

  <enum id='com.canonical.desktop.interface.ScrollbarMode'>
    <value nick='normal' value='0'/>
    <value nick='overlay-auto' value='1'/>
    <value nick='overlay-pointer' value='2'/>
    <value nick='overlay-touch' value='3'/>
  </enum>
  <schema id='com.canonical.desktop.interface' path='/com/canonical/desktop/interface/'>
    <key name='scrollbar-mode' enum='com.canonical.desktop.interface.ScrollbarMode'>
      <default>'overlay-auto'</default>
    </key>
  </schema>

  <schema id="com.canonical.unity-gtk-module" path="/com/canonical/unity-gtk-module/">
    <key name="blacklist" type="as">
      <default>[]</default>
    </key>
    <key name="whitelist" type="as">
      <default>[]</default>
    </key>
  </schema>

  <schema id="org.gnome.desktop.wm.preferences" path="/org/gnome/desktop/wm/preferences/">
    <key type="s" name="theme">
      <default>'Ambiance'</default>
    </key>
    <key type="s" name="titlebar-font">
      <default>'Ubuntu Bold 11'</default>
    </key>
    <key type="b" name="titlebar-uses-system-font">
      <default>false</default>
    </key>
  </schema>

  <schema id="org.gnome.desktop.interface" path="/org/gnome/desktop/interface/">
    <key type="u" name="scaling-factor">
      <default>1</default>
    </key>
    <key type="d" name="text-scaling-factor">
      <default>1.0</default>
    </key>
    <key type="s" name="font-name">
      <default>'Ubuntu 11'</default>
    </key>
    <key type="i" name="cursor-size">
      <default>24</default>
    </key>
  </schema>

  <schema id="com.canonical.unity-greeter" path="/com/canonical/unity-greeter/">
    <key type="s" name="logo">
      <default>'/usr/share/unity-greeter/logo.png'</default>
    </key>
    <key type="s" name="font-name">
      <default>'Ubuntu 11'</default>
    </key>
    <key type="s" name="background">
      <default>'/usr/share/backgrounds/warty-final-ubuntu.png'</default>
    </key>
    <key type="s" name="background-color">
      <default>'#2C001E'</default>
    </key>
    <key type="b" name="draw-user-backgrounds">
      <default>true</default>
    </key>
    <key type="b" name="draw-grid">
      <default>true</default>
    </key>
    <key type="b" name="show-hostname">
      <default>true</default>
    </key>
  </schema>

  <schema path="/org/gnome/desktop/screensaver/" id="org.gnome.desktop.screensaver" gettext-domain="gsettings-desktop-schemas">
    <key type="b" name="idle-activation-enabled">
      <default>true</default>
    </key>
    <key type="b" name="lock-enabled">
      <default>true</default>
    </key>
    <key type="u" name="lock-delay">
      <default>0</default>
    </key>
    <key type="b" name="ubuntu-lock-on-suspend">
      <default>true</default>
    </key>
  </schema>

  <schema path="/org/gnome/desktop/lockdown/" id="org.gnome.desktop.lockdown">
    <key type="b" name="disable-lock-screen">
      <default>false</default>
    </key>
  </schema>

  <schema path="/org/gnome/desktop/a11y/applications/" id="org.gnome.desktop.a11y.applications">
    <key type="b" name="screen-keyboard-enabled">
      <default>false</default>
    </key>
    <key type="b" name="screen-reader-enabled">
      <default>false</default>
    </key>
  </schema>

  <schema id="org.gnome.settings-daemon.plugins.media-keys" path="/org/gnome/settings-daemon/plugins/media-keys/">
    <key type="s" name="volume-mute">
      <default>'XF86AudioMute'</default>
    </key>
    <key type="s" name="volume-down">
      <default>'XF86AudioLowerVolume'</default>
    </key>
    <key type="s" name="volume-up">
      <default>'XF86AudioRaiseVolume'</default>
    </key>
  </schema>

  <schema id="org.gnome.desktop.wm.keybindings" path="/org/gnome/desktop/wm/keybindings/">
    <key type="as" name="switch-input-source-backward">
      <default>["&lt;Shift&gt;&lt;Super&gt;space"]</default>
    </key>
    <key type="as" name="switch-input-source">
      <default>["&lt;Super&gt;space"]</default>
    </key>
  </schema>
</schemalist>
